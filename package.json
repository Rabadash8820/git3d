{
  "name": "git3d",
  "version": "0.0.0",
  "description": "A tool for visualizing git commit graphs in 3D",
  "private": true,
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/node": "^16.6.1",
    "@types/webpack-dev-server": "^3.11.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "prettier": "^2.3.2",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.0",
    "typescript": "^4.1.3",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^3.11.2"
  },
  "scripts": {
    "compile": "tsc --build src/",
    "recompile": "npm run clean  && npm run compile",
    "compile:watch": "npm run compile -- --watch",
    "build": "webpack build --mode ${NODE_ENV:-development}",
    "build:watch": "webpack watch --mode ${NODE_ENV:-development}",
    "rebuild": "npm run clean && npm run build",
    "serve": "webpack serve --mode ${NODE_ENV:-development}",
    "clean": "rm -rf dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rabadash8820/git3d.git"
  },
  "keywords": [
    "git",
    "3d",
    "commits",
    "graph",
    "three.js"
  ],
  "author": "Dan Vicarel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Rabadash8820/git3d/issues"
  },
  "homepage": "https://github.com/Rabadash8820/git3d#readme"
}
