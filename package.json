{
  "name": "git3d",
  "version": "0.0.0",
  "description": "A tool for visualizing git commit graphs in 3D",
  "private": true,
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "http-server": "^0.12.3",
    "prettier": "^2.3.2",
    "ts-loader": "^9.2.5",
    "typescript": "^4.1.3",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.8.0"
  },
  "scripts": {
    "generate-cert": "openssl req -newkey rsa:2048 -new -nodes -x509 -days 3650 -keyout key.pem -out cert.pem -subj '/O=Organization'",
    "prepare": "npm run generate-cert",
    "compile": "tsc --build src/",
    "recompile": "npm run clean  && npm run compile",
    "compile:watch": "npm run compile -- --watch",
    "build": "webpack build --mode ${NODE_ENV:-development}",
    "build:watch": "webpack watch --mode ${NODE_ENV:-development}",
    "rebuild": "npm run clean && npm run build",
    "start": "http-server ./dist --ssl --cert cert.pem --key key.pem -c-1",
    "clean": "rm -rf dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rabadash8820/git3d.git"
  },
  "keywords": [
    "git",
    "3d",
    "commits",
    "graph",
    "three.js"
  ],
  "author": "Dan Vicarel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Rabadash8820/git3d/issues"
  },
  "homepage": "https://github.com/Rabadash8820/git3d#readme"
}
